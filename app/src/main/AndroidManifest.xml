<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.managementapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Appointment Management">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAppointment"
            android:label="Create Appointments" />
        <activity
            android:name=".DeleteAppointment"
            android:label="Delete Appointments" />
        <activity
            android:name=".ViewAndEdit"
            android:label="View/Edit Appointments" />
        <activity
            android:name=".MoveAppointment"
            android:label="Move Appointments" />
        <activity
            android:name=".SearchAppointment"
            android:label="Search Appointments" />
        <activity android:name=".MoveAppoCalendar" />
        <activity android:name=".ThesaurusActivity" />
        <activity android:name=".Edit"></activity>
    </application>

</manifest>